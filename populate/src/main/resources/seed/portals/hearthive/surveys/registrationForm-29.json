{
  "stableId" : "registrationForm",
  "version" : 29,
  "publishedVersion" : 5,
  "name" : "Registration_Form",
  "surveyType" : "RESEARCH",
  "answerMappings" : [ ],
  "triggers" : [ ],
  "recur" : false,
  "required" : false,
  "allowAdminEdit" : true,
  "allowParticipantStart" : true,
  "allowParticipantReedit" : true,
  "prepopulate" : false,
  "assignToAllNewEnrollees" : true,
  "assignToExistingEnrollees" : false,
  "autoUpdateTaskAssignments" : false,
  "jsonContent" : {
    "title" : "cardiomyopathy registration",
    "logoPosition" : "right",
    "pages" : [ {
      "name" : "page1",
      "elements" : [ {
        "type" : "html",
        "name" : "descriptionText",
        "html" : "<h3 class=\"text-center fw-bold\">\nWelcome to the Heart Hive!</h3>\n&nbsp;\n\n<p>Please complete this short form to join the registry and set up an account</p>\n\n\n\n"
      }, {
        "type" : "radiogroup",
        "name" : "hh_prereg",
        "title" : "Please select one of the options",
        "description" : "",
        "isRequired" : true,
        "choices" : [ {
          "text" : "I have been diagnosed with cardiomyopathy",
          "value" : "cm"
        }, {
          "text" : "I have a family history of cardiomyopathy, but have not been diagnosed with cardiomyopathy",
          "value" : "fx"
        }, {
          "text" : "I have had genetic testing which revealed an inherited risk for cardiomyopathy",
          "value" : "genetic_risk_of_cm"
        }, {
          "text" : "I have been diagnosed with myocarditis",
          "value" : "myoc"
        }, {
          "text" : "None of the above",
          "value" : "none"
        } ]
      }, {
        "type" : "radiogroup",
        "name" : "over18",
        "title" : "Are you age 18 or older?",
        "description" : "",
        "isRequired" : true,
        "choices" : [ {
          "text" : "Yes",
          "value" : "yes"
        }, {
          "text" : "No",
          "value" : "no"
        } ]
      }, {
        "type" : "html",
        "name" : "privacy_text",
        "title" : "privacy",
        "html" : "I have read and understood how my data will be used as described in the <a href=\"https://thehearthive.org/api/public/portals/v1/hearthive/env/live/siteImages/2/privacy_notice.pdf\" target=\"_blank\">privacy notice.</a>\n"
      }, {
        "type" : "checkbox",
        "name" : "privacy_notice",
        "title" : "Please tick the box to agree",
        "description" : "",
        "isRequired" : true,
        "choices" : [ {
          "text" : "Yes",
          "value" : "yes"
        } ]
      } ]
    } ],
    "calculatedValues" : [ {
      "name" : "qualified",
      "expression" : "(!{hh_prereg} = 'none') and {over18} = 'yes' and {privacy_notice} allof ['yes']",
      "includeIntoResult" : true
    } ]
  }
}